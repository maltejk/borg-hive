[run]
branch = True
disable_warnings = module-not-measured
source = src
omit =
    src/__init__.py
    */manage.py
    */core/wsgi.py
    */core/asgi.py
    */core/settings.py
    */tests/*

[report]
exclude_lines =
    def __repr__
    def __str__
    raise AssertionError
    raise NotImplementedError
    if 0:
    if __name__ == .__main__.:
ignore_errors = True
